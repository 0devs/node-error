{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-sonarts"
    ],
    "jsRules": {},
    "rules": {
      "variable-name": ["allow-leading-underscore"],
      "object-literal-sort-keys": [false],
      "ordered-imports": [false],
      "no-console": [false],
      "no-duplicate-string": [false],
      "no-commented-code": [false]
    },
    "rulesDirectory": []
}
